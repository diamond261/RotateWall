<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Options</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Set full file paths to images on-device. Store images under /var/mobile/Library/RotateWall and enter the exact filename.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enable Lockscreen Wallpaper</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>lockscreenEnabled</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enable Homescreen Wallpaper</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>homescreenEnabled</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Fade Transition</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>fadeEnabled</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Compatibility</string>
			<key>footerText</key>
			<string>Enable if other tweaks rely on lockscreen wallpaper notifications or average color.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Tweak Compatibility</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>default</key>
			<true/>
			<key>key</key>
			<string>compatibilityModeEnabled</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Lock Screen - Portrait</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Portrait Path</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>lockPortraitPath</string>
			<key>placeholder</key>
			<string>/var/mobile/Library/RotateWall/lock-portrait.jpg</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Lock Screen - Landscape</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Landscape Path</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>lockLandscapePath</string>
			<key>placeholder</key>
			<string>/var/mobile/Library/RotateWall/lock-landscape.jpg</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Home Screen - Portrait</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Portrait Path</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>homePortraitPath</string>
			<key>placeholder</key>
			<string>/var/mobile/Library/RotateWall/home-portrait.jpg</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Home Screen - Landscape</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Landscape Path</string>
			<key>defaults</key>
			<string>com.denial.doabarrelwallprefs</string>
			<key>key</key>
			<string>homeLandscapePath</string>
			<key>placeholder</key>
			<string>/var/mobile/Library/RotateWall/home-landscape.jpg</string>
			<key>PostNotification</key>
			<string>com.denial.doabarrelwallprefs/ReloadPrefs</string>
		</dict>
	</array>
	<key>title</key>
	<string>DoABarrelWall</string>
</dict>
</plist>
